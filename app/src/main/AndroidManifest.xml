<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.bwie.majunbao">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/jd_buy_icon"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/jd_buy_icon"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <!--欢迎页面activity-->
        <activity android:name=".ui.activity.WelcomeActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <!--主页面mainactivity-->
        <activity android:name=".ui.activity.MainActivity">

        </activity>
    </application>



    <!--权限-->
    <uses-permission android:name="android.permission.INTERNET"/><!--网络权限-->
    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE"/><!--写入权限-->
    <uses-permission android:name="android.permission.READ_EXTERNAL_STORAGE"/><!--读取权限-->

</manifest>